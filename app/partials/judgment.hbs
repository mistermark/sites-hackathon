<div class="col-md-12">
  <h2>{{html judgment.h2}}</h2>
  <div class="row" style="max-width: 75%; margin: auto;">
    {{#each judgment.criteria}}
    <div class="col-md-6 criteria">
      <i class="{{html this.icon}}"></i>
      <h4>{{html title}}</h4>
      <p>{{html description}}</p>
    </div>
    {{/each}}
  </div>
</div>
